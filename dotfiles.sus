card dotfiles {
     into ~

     .offlineimaprc
     .offlineimap.py
     .msmtprc
     .xinitrc
     .xprofile
     .zshrc

     xmonad-personal/app/xmonad.hs -> .xmonad/xmonad.hs
     vimfx -> .config/vimfx

     gtk-3.0 -> .config/gtk-3.0

     eshell_alias -> .emacs.d/eshell/alias
}

card taffybar {
     into ~/.config/taffybar

     taffybar.hs
     taffybar.rc
}
