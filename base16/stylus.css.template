/* Base16 <%- scheme %> (https://github.com/chriskempson/base16) */
/* Scheme: <%- author %> */

html {
    background: #<%- base["00"]["hex"] %>
    !important;
}

/*----- DEFAULT TEXT, BORDER & BACKGROUND COLORS -----*/
* {
    color: #<%- base["05"]["hex"] %> !important;
    text-shadow: 0 0 3px #<%- base["00"]["hex"] %> !important;
    box-shadow: none !important;
    background-color: transparent !important;
    border-color: #<%- base["04"]["hex"] %> !important;
    border-top-color: #<%- base["04"]["hex"] %> !important;
    border-bottom-color: #<%- base["04"]["hex"] %> !important;
    border-left-color: #<%- base["04"]["hex"] %> !important;
    border-right-color: #<%- base["04"]["hex"] %> !important;
}
body {
    background: transparent !important;
}
*:before,
*:after {
    background-color: transparent !important;
    border-color: #<%- base["04"]["hex"] %> !important;
}
a,
a * {
    color: #<%- base["0D"]["hex"] %> !important;
    text-decoration: none !important;
}
a:hover,
a:hover *,
a:visited:hover,
a:visited:hover *,
span[onclick]:hover,
div[onclick]:hover,
[role=\"link\"]:hover,
[role=\"link\"]:hover *,
[role=\"button\"]:hover *,
[role=\"menuitem\"]:hover,
[role=\"menuitem\"]:hover *,
.link:hover,
.link:hover * {
    color: #<%- base["0C"]["hex"] %> !important;
    text-shadow: 0 0 5px rgba(255, 255, 200, 0.9) !important;
}
a:visited,
a:visited * {
    color: #<%- base["0E"]["hex"] %> !important;
}
a.highlight,
a.highlight *,
a.active,
a.active *,
.selected,
.selected *,
[href=\"#\"] {
    color: #<%- base["0C"]["hex"] %> !important;
    font-weight: bold !important;
}
h1,
h2,
h3,
h4,
h5,
h6,
h1 *,
h2 *,
h3 *,
strong,
[id*=\"headline\"],
[class*=\"headline\"],
[id*=\"header\"],
[class*=\"header\"],
[class*=\"header\"] td {
    color: #<%- base["06"]["hex"] %> !important;
}
a h1,
a h2,
a h3,
a h4,
a h5,
a h6,
h1 a,
h2 a,
h3 a,
a strong,
a[id*=\"headline\"],
a[class*=\"headline\"],
a[id*=\"header\"],
a[class*=\"header\"] {
    text-decoration: underline !important;
}
[class*=\"error\"],
[class*=\"alert\"],
code,
span[onclick],
div[onclick] {
    color: #<%- base["08"]["hex"] %> !important;
}
 ::-moz-selection {
    background: #<%- base["0D"]["hex"] %> !important;
    color: #<%- base["00"]["hex"] %> !important;
}
 ::selection {
    background: #<%- base["0D"]["hex"] %> !important;
    color: #<%- base["00"]["hex"] %> !important;
}
 :focus {
    outline: none !important;
}

/*----- MENU & CO BACKGROUND-COLORS -----*/
div[style=\"display: block;\"],
div[role=\"navigation\"] {
    background: rgba(0, 0, 0, .5) !important;
}
table {
    background: rgba(40, 30, 30, .6) !important;
    border-radius: 6px !important;
}
table > tbody > tr:nth-child(even),
table > tbody > tr > td:nth-child(even) {
    background-color: rgba(0, 0, 0, .2) !important;
}
iframe,
embed,
nav,
label [onclick],
nav ul,
div[style*=\"position:\"][style*=\"left:\"][style*=\"visible\"],
div[style*=\"z-index:\"][style*=\"left:\"][style*=\"visible\"],
div[style*=\"-moz-user-select\"],
div[role=\"menu\"],
div[role=\"dialog\"],
span[class*=\"script\"] div,
[id*=\"menu\"],
[id*=\"Menu\"],
[class*=\"dropdown\"],
[class*=\"popup\"],
[class=\"title\"],
ul[style*=\"display:\"],
ul[style*=\"visibility:\"] ul,
[id*=\"nav\"] ul,
[class*=\"nav\"] ul,
ul[class*=\"menu\"],
a[onclick][style*=\"display\"],
a[id*=\"ghosteryfirefox\"],
#ghostery-purple-bubble,
#translator-popup,
.menu,
.tooltip,
.hovercard,
.vbmenu_popup {
    background: rgba(5, 5, 5, .9) !important;
    border-radius: 5px;
    box-shadow: 1px 1px 5px #<%- base["00"]["hex"] %> !important;
}
header,
#header,
footer,
#footer {
    background: rgba(19, 19, 19, .9) !important;
    box-shadow: 0 0 5px #<%- base["00"]["hex"] %> !important;
}
body > #dialog,
body > .xenOverlay {
    background: rgba(19, 19, 19, .96) !important;
    background-clip: padding-box !important;
    box-shadow: 0 0 15px #000, inset 0 0 0 1px rgba(200, 200, 200, .5), inset 0 0 5px #<%- base["00"]["hex"] %> !important;
    border: 10px solid rgba(99, 99, 99, .7) !important;
    border-radius: 0 !important;
}
[id*=\"overlay\"],
[id*=\"lightbox\"],
blockquote {
    background-color: rgba(35, 35, 35, .9) !important;
    border-radius: 5px;
}
pre,
dl,
.Message code {
    background-color: rgba(5, 5, 5, .5) !important;
}

/*----- DEFAULT BUTTONS, SEARCHBOXES & CO -----*/
input,
select,
button,
[role=\"button\"],
a.button,
a.submit,
a.BigButton,
a.TabLink,
.install[onclick] {
    -moz-appearance: none !important;
    -webkit-appearance: none !important;
    transition: border-color 0.3s !important;
    background: #<%- base["01"]["hex"] %> !important;
    color: #<%- base["06"]["hex"] %> !important;
    text-shadow: 0 1px #<%- base["01"]["hex"] %> !important;
    border: 2px solid #<%- base["03"]["hex"] %> !important;
    border-radius: 4px !important;
    box-shadow: 0 0 2px rgba(0, 0, 0, .9) !important;
}
a[href=\"javascript:;\"],
a[class*=\"button\"]:not(:empty),
a[id*=\"button\"]:not(:empty),
a[id*=\"Button\"]:not(:empty),
div[class*=\"button\"][onclick] {
    transition: border-color 0.3s !important;
    background: #<%- base["01"]["hex"] %> !important;
    color: #<%- base["06"]["hex"] %> !important;
    text-shadow: 0 1px #<%- base["01"]["hex"] %> !important;
    border-color: #<%- base["03"]["hex"] %> !important;
    box-shadow: 0 0 2px rgba(0, 0, 0, .9) !important;
}
a[href=\"javascript:;\"]:hover,
a[class*=\"button\"]:not(:empty):hover,
a[id*=\"button\"]:hover,
a[id*=\"Button\"]:not(:empty):hover,
div[class*=\"button\"][onclick]:hover {
    background: #<%- base["02"]["hex"] %> !important;
    color: #<%- base["07"]["hex"] %> !important;
}
input *,
select *,
button *,
a.button *,
a.submit * {
    color: #<%- base["06"]["hex"] %> !important;
    text-shadow: none !important;
}
input:hover,
input[type=\"button\"]:hover,
select:hover,
button:hover,
[role=\"button\"]:hover,
a.button:hover,
a.submit:hover,
a.BigButton:hover,
a.TabLink:hover {
    border: 2px solid #<%- base["03"]["hex"] %> !important;
    border-top-color: #<%- base["03"]["hex"] %> !important;
    border-bottom-color: #<%- base["03"]["hex"] %> !important;
    border-left-color: #<%- base["03"]["hex"] %> !important;
    border-right-color: #<%- base["03"]["hex"] %> !important;
}
input:focus,
select:focus {
    box-shadow: 0 0 5px #<%- base["0D"]["hex"] %> !important;
}
input *:hover * {
    color: #<%- base["0C"]["hex"] %> !important;
    text-shadow: 0 0 2px #<%- base["0C"]["hex"] %> !important;
}
input[disabled],
select[disabled],
button[disabled],
input[disabled]:hover,
select[disabled]:hover,
button[disabled]:hover,
input[disabled]:focus,
select[disabled]:focus,
button[disabled]:focus {
    opacity: 0.5 !important;
    border-color: #<%- base["02"]["hex"] %> !important;
}

